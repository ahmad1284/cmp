{
  "name": "cmp",
  "version": "1.0.0",
  "description": "A website for classic media production",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch ./src/_sass:src/_css",
    "build:sass": "sass ./src/_sass:src/_css",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "build:eleventy": "npx @11ty/eleventy",
    "dev": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy --serve",
    "debughard": "DEBUG=Dev:Eleventy* npx @11ty/eleventy --serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmad1284/cmp.git"
  },
  "author": "Ahmad Abdullah",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ahmad1284/cmp/issues"
  },
  "homepage": "https://github.com/ahmad1284/cmp#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "sass": "^1.52.3"
  }
}
